{
	"name": "cube-chaos-semantic-highlighter",
	"displayName": "Cube Chaos Semantic Highlighter",
	"description": "Semantic highlighter for Cube Chaos mod files.",
	"version": "0.1.2",
	"publisher": "Thk",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Thk54/Cube-Chaos-Semantic-Highlighter.git"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/initialize.js",
	"contributes": {
		"languages": [
			{
				"id": "chaos-script",
				"aliases": [
					"Chaos Script",
					"chaos"
				],
				"extensions": [
					".chaos",
					".txt",
					".cc.txt",
					".c.txt",
					".chaos.txt"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "chaos-script",
				"scopeName": "source.chaos",
				"path": "./syntaxes/chaos-script.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0"
	}
}
