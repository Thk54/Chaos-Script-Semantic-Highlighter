{
	"name": "cube-chaos-semantic-highlighter",
	"displayName": "Cube Chaos Semantic Highlighter",
	"description": "Semantic highlighter for Cube Chaos mod files.",
	"version": "0.0.2",
	"publisher": "Thk",
	"private": true,
	"license": "MIT",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "cubechaos",
				"aliases": [
					"Cube Chaos",
					"cubechaos"
				],
				"extensions": [
					".chaos",
					".txt",
					".chaos.txt"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "cubechaos",
				"scopeName": "text.cubechaos",
				"path": "./syntaxes/cubechaos.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"test": "sh ./scripts/e2e.sh"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0"
	}
}
